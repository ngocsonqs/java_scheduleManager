package jp.co.dhw.osaka.scheduler.user;

import jp.co.whizz_tech.ocean.cui.CuiAppBase;

/**
 * ユーザ登録を行うクラスです。
 * @author bangoku
 * @date 2016/05/16
 */
public class UserRegister extends CuiAppBase {

	/**
	 * ユーザ登録を行う
	 */
	@Override
	public void execute() {
		String username = inputStr("ユーザ名を入力してください。", "入力可能な文字列の最大長を超えています。", 16);
	}
}
